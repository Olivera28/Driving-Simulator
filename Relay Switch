int RELAY_PIN = 7;
int PMETER_PIN = A0;
int val = 0;

void setup() {
  // put your setup code here, to run once:
  Serial.begin(9600);
  pinMode(RELAY_PIN, OUTPUT);
}

void loop() {
  // put your main code here, to run repeatedly:f
  val = analogRead(PMETER_PIN);
  Serial.println(val);
  // Relay on and off for certain amount of time
  digitalWrite(RELAY_PIN, HIGH);
  delay(2000);
  digitalWrite(RELAY_PIN, LOW);
  delay(2000);
}
